{
  "apps": [
    {
      "name": "queue-backend",
      "cwd": "./queue-backend/",
      "script": "node",
      "args": "./dist/src/main",
      "env": {
        "NODE_ENV": "production"
      }
    },
    {
      "name": "queue-frontend",
      "cwd": "./queue-frontend/",
      "script": "yarn next",
      "args": "start",
      "log_date_format": "DD-MM-YYYY HH:mm:ss Z",
      "env": {
        "PORT": "6001",
        "NODE_ENV": "production"
      }
    }
  ]
}
